{
  "name": "mermaid-zenuml",
  "version": "9.2.2",
  "description": "Mermaid integration for ZenUML, a text to sequence diagram renderer. For more information, please checkout https://ZenUML.com",
  "main": "dist/mermaid-zenuml.core.mjs",
  "module": "dist/mermaid-zenuml.core.mjs",
  "types": "dist/detector.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/mermaid-zenuml.core.mjs",
      "types": "./dist/detector.d.ts"
    },
    "./*": "./*"
  },
  "keywords": [
    "diagram",
    "markdown",
    "zenuml",
    "mermaid"
  ],
  "scripts": {
    "prepublishOnly": "pnpm -w run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mermaid-js/mermaid.git"
  },
  "author": "",
  "license": "MIT",
  "standard": {
    "ignore": [
      "**/parser/*.js",
      "dist/**/*.js",
      "cypress/**/*.js"
    ],
    "globals": [
      "page"
    ]
  },
  "homepage": "https://github.com/mermaid-js/mermaid#readme",
  "dependencies": {
    "@zenuml/core": "^2.0.12"
  },
  "devDependencies": {
    "concurrently": "^7.5.0",
    "mermaid": "workspace:*",
    "rimraf": "^3.0.2"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ]
}
